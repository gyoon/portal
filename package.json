{
  "name": "gyoon",
  "version": "1.1.0",
  "description": "A personal portfolio blog.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.dev.js",
    "serve": "cross-env NODE_ENV=production node server.prod.js",
    "watch": "webpack --watch",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --colors && npm run build_mv",
    "build_mv": "mv dist/portal.html dist/index.html && mv dist/portal/* dist/ && rm dist/portal -r",
    "clean": "rm -rf dist",
    "deploy": "git-directory-deploy --directory dist/ --branch gh-pages",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gyoon/portal.git"
  },
  "keywords": [
    "personal",
    "blog"
  ],
  "author": "gyoon <woosys25@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gyoon/portal/issues"
  },
  "homepage": "https://github.com/gyoon/portal#readme",
  "dependencies": {
    "history": "^2.0.0",
    "parse": "^1.6.0",
    "parse-react": "^0.5.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.5",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "appcache-webpack-plugin": "^1.2.1",
    "async-props": "^0.3.2",
    "axios": "^0.15.2",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.0",
    "jsonp": "^0.2.0",
    "jwt-decode": "^2.0.1",
    "object.assign": "^4.0.3",
    "react-addons-css-transition-group": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-masonry-component": "^4.2.2",
    "react-static-webpack-plugin": "^0.4.1",
    "react-syntax-highlighter": "^2.0.4",
    "react-transform-hmr": "^1.0.2",
    "redux-devtools": "^3.0.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.1"
  }
}
